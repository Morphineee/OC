 md Zadanie1 
 cd Zadanie1 
 md A1 
 md A2 
 md A3 
 copy "C:\Windows\*.ini" A1 
 cd "C:\Windows" dir /b > "D:\Zadanie1\A2\file.txt" 
 cd "D:\Zadanie1" 
 D:\Zadanie1\xcopy A1 A3 && xcopy A2 A3  
 cd D:\Zadanie1\A1
 D:\Zadanie1\A1\REN *.ini ~ *.bak 
 cd D:\Zadanie1\A3 
 set >  D:\Zadanie1\A3\set.txt 
 set myname = Novikova
 set > D:\Zadanie1\A3\new_set.txt
